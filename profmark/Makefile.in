CC       = @CC@
CFLAGS   = @CFLAGS@
CPPFLAGS = @CPPFLAGS@
LDFLAGS  = @LDFLAGS@
SIMDFLAGS= @SIMD_CFLAGS@
DEFS     = @DEFS@
LIBS     = @LIBS@ -lm
MPILIBS  = @MPILIBS@
IMPLDIR  = impl_@IMPL_CHOICE@

SRCDIR   = ../src
ESLDIR   = ../easel

PROGS    = create-profmark\
	   rocplot

all: ${PROGS} 
	cd ${ESLDIR};            ${MAKE} easel_lib
	cd ${SRCDIR}/${IMPLDIR}; ${MAKE} impl_lib
	cd ${SRCDIR};            ${MAKE} hmmer_lib
	cd ${SRCDIR};            ${MAKE} hmmer_progs
	${MAKE} profmark_progs

profmark_progs: ${PROGS}

.c.o: 
	${CC} -I ${ESLDIR} ${CFLAGS} ${SIMDFLAGS} ${DEFS} -c $<		

${PROGS}: @EXEC_DEPENDENCY@  ${SRCDIR}/libhmmer.a ${ESLDIR}/libeasel.a
	${CC} ${CFLAGS} ${DEFS} ${LDFLAGS} -I ${ESLDIR} -L ${ESLDIR} -o $@ $@.o ${ESLDIR}/libeasel.a ${LIBS}

distclean:
	make clean
	-rm -f Makefile

clean:
	-rm -f *.o *~  ${PROGS} TAGS
	-rm -f *.gcno

