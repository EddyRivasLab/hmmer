CC       = @CC@
CFLAGS   = @CFLAGS@
CPPFLAGS = @CPPFLAGS@
LDFLAGS  = @LDFLAGS@
SIMDFLAGS= @SIMD_CFLAGS@
DEFS     = @DEFS@
LIBS     = @LIBS@ -lm
MPILIBS  = @MPILIBS@

SRCDIR   = ../src
ESLDIR   = ../easel

PROGS    = concatdb

all: ${PROGS} 

.c.o: 
	${CC} -I ${ESLDIR} ${CFLAGS} ${SIMDFLAGS} ${DEFS} -c $<		

${PROGS}: @EXEC_DEPENDENCY@  ${SRCDIR}/libhmmer.a ${ESLDIR}/libeasel.a
	${CC} ${CFLAGS} ${DEFS} ${LDFLAGS} -I ${ESLDIR} -L ${ESLDIR} -o $@ $@.o ${ESLDIR}/libeasel.a ${LIBS}

