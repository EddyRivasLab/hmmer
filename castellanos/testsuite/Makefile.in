IMPLDIR  = impl_@IMPL_CHOICE@
ESLDIR   = ../easel

all:  

check: 
	@cd ../src/;           ${MAKE} check
	@cd ../src/${IMPLDIR}; ${MAKE} check
	@echo 
	@echo Running HMMER test suite...
	@echo
	@${ESLDIR}/devkit/sqc 2 testsuite.sqc . ../src/ ../src/${IMPLDIR}

clean:
	-rm -f *.gcno *.gcda *.gcov
	-rm -f *.o *~ 

distclean:
	make clean
	-rm -f Makefile
