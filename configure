fi
ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu


fi




# Support for vector implementations
#
# If we were explicitly told to enable one ($enable_foo="yes") and we
# can't, fail with an error.
#
# If we're autodetecting ($enable_foo="check"), set $enable_foo to the
# result ("yes" or "no").
#
# If vector support "foo" is enabled:
#    - define preprocessor symbol eslENABLE_FOO (esl_config.h.in, p7_config.h.in)
#    - set output variable FOO_CFLAGS, if needed (Makefile.in)
#    - set shell variable $enable_foo to "yes"
#    - set shell variable $impl_choice to "foo"
# and if vector support is available (regardless of whether we
# decide to enable it), the autoconf macros:
#    - set shell variable $esl_have_foo to "yes"
#    - set shell var $esl_foo_cflags to any necessary compiler flags
#
if test "$enable_vmx" = "yes" || test "$enable_vmx" = "check"; then

  { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether $CC can compile our Altivec/VMX code" >&5
$as_echo_n "checking whether $CC can compile our Altivec/VMX code... " >&6; }
  esl_have_vmx=no

  if test "x$VMX_CFLAGS" != x; then
    esl_vmx_try_flags=$VMX_CFLAGS
  else
    esl_vmx_try_flags="none -maltivec -faltivec"
  fi

  save_CFLAGS=$CFLAGS
  for esl_vmx_cflags in $esl_vmx_try_flags; do
    case $esl_vmx_cflags in
      none) CFLAGS=$save_CFLAGS;;
      *)    CFLAGS="$save_CFLAGS $esl_vmx_cflags";;
    esac

    cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

#include <altivec.h>
#include <stdint.h>
int stub_vmx(void) {
vector signed int v1,v2;
union { vector signed int v; int32_t x[4]; } v3;
v1   = vec_splat_s32(6);
v2   = vec_splat_s32(12);
v3.v = vec_add(v1,v2);
return (int) v3.x[0];}
int main(void) { if (stub_vmx() != 18) return 1; else return 0; }

_ACEOF
if ac_fn_c_try_link "$LINENO"; then :
   esl_have_vmx=yes; break;
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
  done
  CFLAGS=$save_CFLAGS

  if test "$esl_have_vmx" = "no"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    esl_vmx_cflags=
  else
    if test "$esl_vmx_cflags" = "none"; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
      esl_vmx_cflags=
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes, with $esl_vmx_cflags" >&5
$as_echo "yes, with $esl_vmx_cflags" >&6; }
    fi
  fi

  if test "x$esl_have_vmx" = xyes; then :


$as_echo "#define eslENABLE_VMX 1" >>confdefs.h

    HMMERIMPLLIB="impl_vmx/libhmmerimpl.a"

    VMX_CFLAGS=$esl_vmx_cflags

    enable_vmx=yes
    impl_choice=vmx

else

    if test "$enable_vmx" = "yes"; then
      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Unable to compile our Altivec/VMX implementations. Try another compiler?
See \`config.log' for more details" "$LINENO" 5; }
    fi
    enable_vmx=no

fi

fi

if test "$enable_sse" = "yes" || test "$enable_sse" = "check"; then

  { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether $CC can compile our SSE code" >&5
$as_echo_n "checking whether $CC can compile our SSE code... " >&6; }
  esl_have_sse=no

  if test "x$SSE_CFLAGS" != x; then
    esl_sse_try_flags=$SSE_CFLAGS
  else
    esl_sse_try_flags="none -msse2"
  fi

  save_CFLAGS=$CFLAGS
  for esl_sse_cflags in $esl_sse_try_flags; do
    case $esl_sse_cflags in
      none) CFLAGS=$save_CFLAGS;;
      *)    CFLAGS="$save_CFLAGS $esl_sse_cflags";;
    esac

    cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

#include <x86intrin.h>
#include <stdint.h>
int stub_sse(void) {
__m128i v1 = _mm_set1_epi32(42);
__m128i v2 = _mm_set1_epi32(86);
union { __m128i v; int32_t x[4]; } v3;
v3.v = _mm_add_epi32(v1, v2);
return (int) v3.x[0];
}
int main(void) { if (stub_sse() != 128) return 1; else return 0; }

_ACEOF
if ac_fn_c_try_link "$LINENO"; then :
   esl_have_sse=yes; break;
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
  done

  CFLAGS=$save_CFLAGS
  if test "$esl_have_sse" = "no"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    esl_sse_cflags=
  else
    if test "$esl_sse_cflags" = "none"; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
      esl_sse_cflags=
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes, with $esl_sse_cflags" >&5
$as_echo "yes, with $esl_sse_cflags" >&6; }
    fi
  fi

  if test "x$esl_have_sse" = xyes; then :


$as_echo "#define eslENABLE_SSE 1" >>confdefs.h

    HMMERIMPLLIB="impl_sse/libhmmerimpl.a"

    SSE_CFLAGS=$esl_sse_cflags

    enable_sse=yes
    impl_choice=sse

else

    if test "$enable_sse" = "yes"; then
      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Unable to compile our SSE implementations. Try another compiler?
See \`config.log' for more details" "$LINENO" 5; }
    fi
    enable_sse=no

fi

fi

