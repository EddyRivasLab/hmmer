  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $HAVE_GZIP" >&5
$as_echo "$HAVE_GZIP" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
