      { $as_echo "$as_me:${as_lineno-$LINENO}: HMMER3 requires SSE or VMX vector instructions." >&5
$as_echo "$as_me: HMMER3 requires SSE or VMX vector instructions." >&6;}
      { $as_echo "$as_me:${as_lineno-$LINENO}: Supported platforms are x86 (Intel/AMD) and PowerPC." >&5
$as_echo "$as_me: Supported platforms are x86 (Intel/AMD) and PowerPC." >&6;}
      as_fn_error $? "No supported vectorization found for your machine." "$LINENO" 5
      ;;
esac
IMPL_CHOICE=$impl_choice




